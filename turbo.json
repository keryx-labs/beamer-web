{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "format": {
      "outputs": [
        ".cache/.prettiercache"
      ],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": [
        "^topo"
      ],
      "outputs": [
        ".cache/.eslintcache"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo"
      ],
      "outputs": [
        ".cache/tsbuildinfo.json"
      ]
    }
  }
}
